{
  "name": "markdown-spec-formatter",
  "main": "./lib/markdown-spec-formatter.js",
  "version": "1.3.3",
  "description": "A markdown formatter for Atom based on npm module: @quilicicf/markdown-formatter",
  "keywords": [
    "markdown",
    "formatter"
  ],
  "repository": "https://github.com/quilicicf/markdown-spec-formatter.git",
  "bugs": {
    "url": "https://github.com/quilicicf/markdown-spec-formatter/issues"
  },
  "homepage": "https://github.com/quilicicf/markdown-spec-formatter#readme",
  "author": "quilicicf <quilicicf@gmail.com> (https://github.com/quilicicf)",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "upgrade-markdown-formatter": "parcel build --no-cache --no-minify --no-autoinstall --no-content-hash --target electron --experimental-scope-hoisting --bundle-node-modules node_modules/@quilicicf/markdown-formatter/lib/formatFromString.js",
    "lint": "eslint './**/*.js'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@quilicicf/markdown-formatter": "4.0.2",
    "snyk": "^1.671.0"
  },
  "devDependencies": {
    "@talend/eslint-config": "3.0.0",
    "eslint": "7.31.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-atom": "0.1.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "parcel-bundler": "1.12.5"
  },
  "snyk": true
}
